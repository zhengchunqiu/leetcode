class Solution {
    public int[] twoSum(int[] nums, int target) {
        int result[]=new int[2];
        int len=nums.length;
        int i,j,sum;
        for(i=0;i<len;i++){
            for(j=i+1;j<len;j++){
                sum=nums[i]+nums[j];
                if(sum==target){
                    result[0]=i;
                    result[1]=j;
                }
            }
        }
       return result;
    }
}
